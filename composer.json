{
    "name": "kaile/yii2-polyfield",
    "description": "Dynamically adds new fields in form.",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","dynamic fields"],
    "license": "BSD",
    "authors": [
        {
            "name": "kaile",
            "email": "fix-06@yandex.ru"
        }
    ],
	"require": {
        "php": ">=5.5.0",
        "yiisoft/yii2": "^2.0",
        "trntv/yii2-datetime-widget": "^1.1",
        "npm-asset/select2-bootstrap-theme": "@beta",
        "bower-asset/collapsible": "^1.2",
        "npm-asset/devbridge-autocomplete": "^1.4",
        "npm-asset/tinymce": "^6.3",
        "npm-asset/tinymce-i18n": "^22.5"
    },
    "config": {
        "process-timeout": 1800,
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "repositories":[
        {
            "type": "git",
            "url": "https://github.com/Kaile/yii2-tinymce.git"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "kaile\\polyfield\\": ""
        }
    }
}
